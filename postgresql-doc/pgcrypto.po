msgid "Default"
msgstr "По умолчанию"

#. +> REL_10 REL9_6_3 REL9_6
#: pgcrypto.xml:261(entry)
msgid "Min"
msgstr "Мин."

#. +> REL_10 REL9_6_3 REL9_6
#: pgcrypto.xml:262(entry)
msgid "Max"
msgstr "Макс."

#. +> REL_10 REL9_6_3 REL9_6
#: pgcrypto.xml:268(entry)
msgid "725"
msgstr "725"

#. +> REL_10 REL9_6_3 REL9_6
#: pgcrypto.xml:269(entry) pgcrypto.xml:374(entry)
msgid "1"
msgstr "1"

#. +> REL_10 REL9_6_3 REL9_6
#: pgcrypto.xml:270(entry)
msgid "16777215"
msgstr "16777215"

