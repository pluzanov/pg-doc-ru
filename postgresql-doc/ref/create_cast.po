msgid ""
"<xref linkend=\"sql-createfunction\"/>, <xref linkend=\"sql-createtype\"/>, "
"<xref linkend=\"sql-dropcast\"/>"
msgstr ""
"<xref linkend=\"sql-createfunction\"/>, <xref linkend=\"sql-createtype\"/>, "
"<xref linkend=\"sql-dropcast\"/>"

