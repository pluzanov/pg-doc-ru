msgid "Examples"
msgstr "Примеры"

#: hstore.xml:486(programlisting)
#, no-wrap
msgid "UPDATE tab SET h = h || hstore('c', '3');"
msgstr "UPDATE tab SET h = h || hstore('c', '3');"

