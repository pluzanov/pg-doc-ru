#: ref/pg_dump.xml:21(refpurpose)
msgid ""
"extract a <placeholder-1/> database into a script file or other archive file"
msgstr ""
"выгрузить базу данных <placeholder-1/> в формате скрипта в файл или архив"

#: ref/pg_dump.xml:28(command)
msgctxt "command"
msgid "pg_dump"
msgstr "pg_dump"

#: ref/pg_dump.xml:29(replaceable)
