msgid ""
"CREATE TEXT SEARCH TEMPLATE <placeholder-1/> (\n"
"    [ INIT = <placeholder-2/> , ]\n"
"    LEXIZE = <placeholder-3/>\n"
")"
msgstr ""
"CREATE TEXT SEARCH TEMPLATE <placeholder-1/> (\n"
