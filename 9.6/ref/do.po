#: ref/do.xml:24(refpurpose)
msgid "execute an anonymous code block"
msgstr "выполнить анонимный блок кода"

#: ref/do.xml:28(replaceable) ref/do.xml:67(replaceable)
msgid "lang_name"
msgstr "имя_языка"

#: ref/do.xml:28(replaceable) ref/do.xml:56(replaceable)
