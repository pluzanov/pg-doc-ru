msgstr "option_name"

#: information_schema.xml:1074(entry) information_schema.xml:2668(entry)
#: information_schema.xml:2777(entry) information_schema.xml:2904(entry)
#: information_schema.xml:6294(entry)
msgid "Name of an option"
msgstr "Имя параметра"

#: information_schema.xml:1078(literal) information_schema.xml:2672(literal)
#: information_schema.xml:2781(literal) information_schema.xml:2908(literal)
#: information_schema.xml:6298(literal)
