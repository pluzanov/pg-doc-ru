#: catalogs.xml:4819(structname) catalogs.xml:6305(structname)
#: catalogs.xml:6451(structname) catalogs.xml:6534(structname)
#: catalogs.xml:6631(structname) catalogs.xml:6705(structname)
#: catalogs.xml:7923(structname) catalogs.xml:8306(structname)
#: catalogs.xml:8379(structname) catalogs.xml:8963(structname)
#: catalogs.xml:9056(structname) catalogs.xml:9507(structname)
#: catalogs.xml:9688(structname) catalogs.xml:10050(structname)
msgid "pg_namespace"
msgstr "pg_namespace"

#: catalogs.xml:1566(literal) catalogs.xml:1933(literal)
#: catalogs.xml:2052(literal) catalogs.xml:2324(literal)
#: catalogs.xml:2654(literal) catalogs.xml:3198(literal)
#: catalogs.xml:4281(literal) catalogs.xml:4379(literal)
#: catalogs.xml:4543(literal) catalogs.xml:4819(literal)
